		"eslint-plugin-storybook": "^0.5.7",
		"husky": "^7.0.4",
		"prettier": "^2.5.1",
		"webpack": "^5.70.0"
	},
	"eslintConfig": {
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	}
